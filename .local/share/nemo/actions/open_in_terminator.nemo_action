[Nemo Action]

 Name=Tilix
 Comment=Open the 'tilix' terminal in the selected folder
 Exec=tilix --working-directory="%F"
 Icon-Name=terminal
 Selection=any
 Extensions=dir;

 Name=Alacritty
 Comment=Open the 'Alacritty' terminal in the selected folder
 Exec=alacritty --working-directory="%F"
 Icon-Name=terminal
 Selection=any
 Extensions=dir;